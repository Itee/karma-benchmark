{
  "name": "karma-benchmark",
  "description": "Continuous JavaScript Performance Monitoring with Benchmark.js and the Karma Runner",
  "version": "1.0.0",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "benchmark": "global:Benchmark"
  },
  "bugs": "https://github.com/JamieMason/karma-benchmark/issues",
  "contributors": [
    "Jacob Rask (https://github.com/jacobrask)",
    "Jared Sprague (https://github.com/Jared-Sprague)",
    "Larry Davis (https://github.com/lazd)",
    "Micha Reiser (https://github.com/MichaReiser)",
    "Oleh Aloshkin (https://github.com/AleshaOleg)"
  ],
  "dependencies": {
    "lodash": "4.17.11",
    "platform": "1.3.5"
  },
  "devDependencies": {
    "benchmark": "2.1.4",
    "browserify": "16.2.3",
    "browserify-shim": "3.8.13",
    "karma": "3.1.4",
    "xo": "0.24.0"
  },
  "files": [
    "dist/",
    "index.js",
    "src/"
  ],
  "homepage": "https://github.com/JamieMason/karma-benchmark",
  "keywords": [
    "benchmark",
    "jsperf",
    "karma",
    "karma-framework",
    "karma-plugin",
    "monitoring",
    "performance"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "benchmark": "^2.1.4",
    "karma": "^3.1.4"
  },
  "repository": "JamieMason/karma-benchmark",
  "scripts": {
    "build": "browserify src/main.js -o dist/karma-benchmark.js",
    "lint": "xo --fix",
    "prebuild": "mkdir -p dist",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "xo"
  },
  "xo": {
    "envs": [
      "browser",
      "jasmine",
      "node"
    ],
    "globals": [
      "_"
    ],
    "space": 2
  }
}
